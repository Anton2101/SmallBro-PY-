{
	"manifest_version": 2,

	"name": "Search Url",
	"version": "1.0",
	
	"icons":{
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"content_scripts" : [
		{
			"matches" : ["<all_urls>"],
			"js" : ["js/jquery-3.3.1.min.js", "js/background.js"],
			"run_at" : "document_end"
		}
	]
}